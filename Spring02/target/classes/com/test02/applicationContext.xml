<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean으로 2명 등록 -->
	<bean id="hong" class="com.test02.Address">
		<constructor-arg name="name" value="홍길동"/>
		<constructor-arg name="addr" value="강남구"/>
		<constructor-arg name="phone" value="010-1234-1234"/>
	</bean>
	<bean id="lee" class="com.test02.Address">
		<constructor-arg index="0" value="이순신"/>
		<constructor-arg index="1" value="도봉구"/>
		<constructor-arg index="2" value="010-4334-3334"/>
	</bean>
</beans>
